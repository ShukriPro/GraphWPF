<Window x:Class="GraphWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphWPF" xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="auto">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <oxy:PlotView Model="{Binding PMSGraph}" Height="300" Width="450"/>
            <oxy:PlotView Model="{Binding GISGraph}" Height="300" Width="450"/>
        </StackPanel>
    </Grid>
</Window>
